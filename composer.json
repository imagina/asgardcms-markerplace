{
    "name": "imagina/marketplace-module",
    "description": "Marketplace Module transforms your online e-commerce store into the multi-functional and interactive marketplace.",
    "type": "asgard-module",
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "composer/installers": "~1.0",
        "idavoll/core-module": "~3.0",
        "kalnoy/nestedset":"~5.0.*",
        "imagina/icommece":">=1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "3.5.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Marketplace\\": ".",
            "Modules\\": "Modules/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Desarrollo Imagina",
            "email": "soporte@imaginacolombia.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Marketplace\\Providers\\MarketplaceServiceProvider"
            ],
            "aliases": {
                
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Marketplace\\": ""
        }
    }
}
